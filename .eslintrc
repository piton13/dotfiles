{
    "env": {
        "angular": true
    },
    "rules": {
        "comma-dangle": 2,
        "no-console": 1,
        "no-debugger": 2,
        "no-empty": 1,
        "no-extra-semi": 1,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 1,
        "no-unreachable": 2,
        "use-isnan": 2,
        "block-scoped-var": 2,
        "curly": 1,
        "default-case": 2,
        "eqeqeq": 1,
        "no-alert": 2,
        "no-else-return": 2,
        "no-labels": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 1,
        "no-redeclare": 2,
        "no-self-compare": 2,
        "vars-on-top": 2,
        "wrap-iife": [2, "any"],
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-unused-vars": 1,
        "brace-style": [2, "1tbs", {
            "allowSingleLine": true
        }],
        "comma-spacing": [2, {
            "before": false,
            "after": true
        }],
        "comma-style": [2, "last"],
        "key-spacing": [2, {
            "beforeColon": false,
            "afterColon": true,
            "mode": "strict"
        }],
        "new-cap": 2,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-multiple-empty-lines": [2, {
            "max": 1,
            "maxBOF": 0
        }],
        "semi-spacing": 1,
        "no-spaced-func": 2,
        "no-trailing-spaces": 1,
        "no-extra-parens": 2,
        "one-var": [2, "always"],
        "padded-blocks": [1, "never"],
        "quotes": [1, "single"],
        "semi": [2, "always", {
            "omitLastInOneLineBlock": true
        }],
        "space-before-function-paren": [2, "never"],
        "keyword-spacing": 2,
        "space-before-blocks": 2,
        "object-curly-spacing": [2, "never"],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": [2, {
            "words": true,
            "nonwords": false
        }],
        "spaced-comment": 1
    }
}
